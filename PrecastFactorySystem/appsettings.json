{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "Identity": {
        "SignIn": {
            "RequireConfirmedAccount": false
        },

        "Password": {
            "RequireDigit": false,
            "RequireNonAlphanumeric": false,
            "RequireUppercase": false
        },
        "AllowedHosts": "*",
        "ConnectionStrings": {
            "PrecastFactoryDbContextConnection": "Server=.;Database=PrecastFactorySystemDb;Trusted_Connection=True;MultipleActiveResultSets=true"
        }
    }
}